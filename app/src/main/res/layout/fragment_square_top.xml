<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@null"
    android:clipChildren="false"
    android:orientation="vertical"
    app:divider="@drawable/line2"
    app:showDividers="middle"
    android:clickable="true"
    android:paddingBottom="@dimen/dp12"
    >

    <RelativeLayout
        android:id="@+id/life_circle_relat"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp55"
        android:background="@drawable/msg_list_selector_background_ripple">

        <ImageView
            android:id="@+id/life_circle_image"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:src="@drawable/life_circle"/>

        <TextView
            android:id="@+id/life_circle2_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:layout_toRightOf="@id/life_circle_image"
            android:text="@string/life_circle2"
            android:textColor="@color/msg_list_text_color"
            android:textSize="15sp"/>

        <TextView
            android:id="@+id/main_tab_three_tv"
            tools:text="99+"
            tools:visibility="visible"
            android:layout_width="@dimen/dp16"
            android:layout_height="@dimen/dp16"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:layout_toRightOf="@+id/life_circle2_tv"
            android:background="@drawable/message_num_bg_by_has_push"
            android:gravity="center"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_11_text_size"/>

        <ImageView
            android:layout_width="7dp"
            android:layout_height="12dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:src="@drawable/find_return"/>

    </RelativeLayout>

    <android.support.v7.widget.LinearLayoutCompat
        android:id="@+id/video_ll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:divider="@drawable/line"
        app:showDividers="middle"
        android:background="@drawable/msg_list_selector_background_ripple"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/douyin_relat"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <ImageView
                android:id="@+id/douyin_image"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp20"
                android:src="@drawable/short_video"/>

            <TextView
                android:id="@+id/douyin_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_toRightOf="@id/douyin_image"
                android:text="@string/douyin"
                android:textColor="@color/msg_list_text_color"
                android:textSize="15sp"/>

            <ImageView
                android:layout_width="7dp"
                android:layout_height="12dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:src="@drawable/find_return"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/video_conference_relat"
            android:layout_width="match_parent"
            android:layout_height="55dp">
            <ImageView
                android:id="@+id/video_conference_image"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp20"
                android:src="@drawable/tv_meeting"/>

            <TextView
                android:id="@+id/video_conference_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_toRightOf="@id/video_conference_image"
                android:text="@string/chat_video_conference"
                android:textColor="@color/msg_list_text_color"
                android:textSize="15sp"/>

            <ImageView
                android:layout_width="7dp"
                android:layout_height="12dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:src="@drawable/find_return"/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/live_chat_relat"
            android:layout_width="match_parent"
            android:layout_height="55dp">

            <ImageView
                android:id="@+id/live_chat_image"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp20"
                android:src="@drawable/live_video"/>

            <TextView
                android:id="@+id/live_chat_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp20"
                android:layout_toRightOf="@id/live_chat_image"
                android:text="@string/live_chat"
                android:textColor="@color/msg_list_text_color"
                android:textSize="15sp"/>

            <ImageView
                android:layout_width="7dp"
                android:layout_height="12dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="12dp"
                android:src="@drawable/find_return"/>


        </RelativeLayout>

    </android.support.v7.widget.LinearLayoutCompat>

    <RelativeLayout
        android:id="@+id/find_nearby_relat"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp55"
        android:background="@drawable/msg_list_selector_background_ripple">

        <ImageView
            android:id="@+id/find_nearby_image"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:src="@drawable/find_nearby"/>

        <TextView
            android:id="@+id/find_nearby_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:layout_toRightOf="@id/find_nearby_image"
            android:text="@string/near_person"
            android:textColor="@color/msg_list_text_color"
            android:textSize="15sp"/>

        <ImageView
            android:layout_width="7dp"
            android:layout_height="12dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:src="@drawable/find_return"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bjnews_relat"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp55"
        android:background="@drawable/msg_list_selector_background_ripple">

        <ImageView
            android:id="@+id/bjnews_image"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:src="@drawable/bjnews"/>

        <TextView
            android:id="@+id/bjnews_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp20"
            android:layout_toRightOf="@id/bjnews_image"
            android:text="@string/service_number"
            android:textColor="@color/msg_list_text_color"
            android:textSize="15sp"/>

        <ImageView
            android:layout_width="7dp"
            android:layout_height="12dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:src="@drawable/find_return"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/sign_in_red_packet_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp55"
        android:background="@drawable/msg_list_selector_background_ripple"
        >

        <ImageView
            android:id="@+id/my_envelope_iv"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="@dimen/dp20"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:src="@drawable/sign_in_red_packet"/>

        <TextView
            android:id="@+id/my_envelope_tv"
            style="@style/IMTbleLine_text_Me"
            android:layout_toRightOf="@+id/my_envelope_iv"
            android:text="@string/sign_in_red_envelope"/>

        <ImageView
            style="@style/IMTbleLine_ArrowImg.Me"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="12dp"
            android:contentDescription="@string/app_name"/>
    </RelativeLayout>

</android.support.v7.widget.LinearLayoutCompat>